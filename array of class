#include<iostream>
using namespace std;

class emp
{
private:
    int a,b;
public:
    void fix()
    {
        a=200;
        cout<<"employ salary : "<<a<<endl;
        cout<<"enter yor id : "<<endl;
        cin>>b;
    }
    void print()
    {
        cout<<"your id : "<<b<<endl;
    }
};

int main()
{
    emp e[5];
    for(int i=0; i<5; i++)
    {
        e[i].fix();
        e[i].print();

    }
    return 0;
}
