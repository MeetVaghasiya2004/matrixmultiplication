//5 student's 6 subject marks by array
#include <stdio.h>

int main() 
{
    int a[5][6],i,j,n,sum=0;
    for(i=0;i<5;i++)
    {
        sum=0;
        for(j=0;j<6;j++)
        {
            printf("enter mark of a[%d][%d] : ",i+1,j+1);
            scanf("%d",&a[i][j]);
            sum=sum+a[i][j];
        }
        printf("%d\n",sum/6);
    }
        
}
