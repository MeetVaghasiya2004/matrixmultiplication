#include<stdio.h>
void main()
{
    int a,b,c;
    printf("enter a : ");
    scanf("%d",&a);
    printf("enter a : ");
    scanf("%d",&b);
    printf("enter a : ");
    scanf("%d",&c);
    if(a<b && a<c)
    {
        printf("a is min ");
    }
    else if(b<c)
    {
        printf("b is min ");
    }
    else
    {
        printf("c is min ");
    }
}
